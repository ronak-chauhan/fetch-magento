<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="film_roll" model="uom.uom">
        <field name="name">Film Roll</field>
        <field name="category_id" ref="uom.product_uom_categ_unit" />
        <field name="uom_type">bigger</field>
        <field name="factor_inv">100000</field>
    </record>

    <record id="3pl_location" model="stock.location">
        <field name="name">3PL-Location</field>
        <field name="location_id" model="stock.location"
                eval="obj().env.ref('stock.warehouse0').view_location_id.id"/>
        <field name="usage">transit</field>
    </record>

        <function model="magento.configure" name="_mob_def_setting">
                <value model="magento.configure" eval="obj().search([('active', '=', True)])"/>
        </function>
</odoo>